#include<stdio.h>
#include<stdlib.h>
# define n 5
int queue[n];
int front=-1;
int rear=-1;
void enqueue(int);
void dequeue();
void display();
int main()
{
    int choice,item;
    while(1)
    {
        printf("\nMenu\n");
        printf("1.enqueue\n2.dequeue\n3.display\n4.exit\n");
        printf("enter your choice ");
        scanf("%d",&choice);
        switch(choice)
        {
            case 1:printf("enter item ");
                   scanf("%d",&item);
                   enqueue(item);
                   break;
            case 2:dequeue();
                   break;
            case 3:display();
                   break;
            case 4:exit(0);
            default:printf("invalid");
            
        }
    }
}
void dequeue()
{
    if(front==-1&&rear==-1)
    {
        printf("list is empty");
    }
    else if(front==rear)
    {
        printf("delete element is %d",queue[front]);
        front=rear=-1;
    }
    else
    {
        printf("deleted element is %d",queue[front]);
        front=front+1;
    }
    
}
void enqueue(int item)
{
    if(rear==n-1)
    {
        printf("list is full");
    }
    else if(front==-1&&rear==-1)
    {
        front=rear=0;
        queue[rear]=item;
    }
    else
    {
        rear=rear+1;
        queue[rear]=item;
    }
}
void display()
{
int i;
    if(front==-1&rear==-1)
    {
        printf("list is empty");
    }
    else
    {
        for(i=front;i<rear+1;i++)
        {
            printf("%d\t",queue[i]);
        }
    }
}

